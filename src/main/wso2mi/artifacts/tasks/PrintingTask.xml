<?xml version="1.0" encoding="UTF-8"?>
<task class="org.apache.synapse.startup.tasks.MessageInjector" group="synapse.simple.quartz" name="PrintingTask" xmlns="http://ws.apache.org/ns/synapse">
    <trigger interval="5"/>
    <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks" name="message"><message></message></property>
    <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks" name="injectTo" value="sequence"/>
    <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks" name="sequenceName" value="PrintingTaskSequence"/>
</task>